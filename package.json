{
  "name": "sql-ts-knex-generator",
  "version": "0.1.9",
  "main": "./dist/index.js",
  "license": "MIT",
  "description": "Generate TypeScript definitions (https://github.com/rmp135/sql-ts) and Knex.js(https://github.com/knex/knex) query helper functions from database schemas.",
  "bin": "./bin/sql-ts-knex-generator",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/saostad/sql-ts-knex-generator",
    "type": "git"
  },
  "author": {
    "name": "Saeid Ostad"
  },
  "keywords": [
    "knex",
    "typescript",
    "interface",
    "generator",
    "CRUD"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "prebuild": "copyfiles -f ./src/templates/*.handlebars ./dist/templates/",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "jasmine ./__test/{**,}/*.spec.js"
  },
  "dependencies": {
    "handlebars": "^4.7.6",
    "knex": "^0.21.1",
    "yargs": "^15.4.0"
  },
  "devDependencies": {
    "@types/handlebars": "^4.0.39",
    "@types/jasmine": "^3.5.11",
    "@types/knex": "^0.16.1",
    "@types/rewire": "^2.5.28",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fast-node-logger": "^1.4.2",
    "jasmine": "^3.1.0",
    "prettier": "^2.0.5",
    "rewire": "^5.0.0",
    "typescript": "^3.9.6"
  }
}
